{
  "name": "aozisik/outport",
  "description": "Exports Laravel Collections to a single SQLite database",
  "keywords": [
    "laravel",
    "collections",
    "export",
    "sqlite"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmet Ozisik",
      "email": "ozisikahmet@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "4.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "laravel/laravel": "4.2.*",
    "mockery/mockery": "^0.9.5"
  },
  "autoload": {
    "psr-4": {
      "Aozisik\\Outport\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "stable"
}
